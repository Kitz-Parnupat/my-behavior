{
    "toast-load": {
        "prefix": "toast-load",
        "body": [
            "openToastLoad:function(){", 
                "this.fire('toast',{status:'load'});", 
            "}"
        ],
        "description": "toast load"
    },
    "toast-success": {
        "prefix": "toast-success",
        "body": [
                "openToastStatus:function(){",
                    "this.fire('toast',{status:'success',text:'บันทึกสำเร็จ',",
                    "  callback:function(){",
                    "  }",
                " });",
            " }"
        ],
        "description": "toast success"
    },
